    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.25.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^5.0.0",
    "tsc-watch": "^6.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
